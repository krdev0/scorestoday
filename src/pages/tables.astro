---
import Layout from "../layouts/Layout.astro";
import AppLayout from "../layouts/AppLayout.astro";
import Header from "../components/Header.astro";
import Sidebar from "../components/app/Sidebar.astro";
import Articles from "../components/app/Articles.astro";
import SportsNav from "../components/SportsNav.astro";
import Article from "../components/app/Article.astro";
import TableWrap from "../components/table/TableWrap.astro";
import Table from "../components/table/Table.astro";
---

<Layout title="Welcome to LiveScores">
    <main>
        <Header />
        <SportsNav/>

        <AppLayout>
            <Sidebar />

            <TableWrap>
                <Table/>
            </TableWrap>

            <Articles>
				<Article/>
				<Article/>
				<Article/>
				<Article/>
				<Article/>
			</Articles>
        </AppLayout>
    </main>
</Layout>
